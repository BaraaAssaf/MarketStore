@model MarketStore.Models.Store

@{
    ViewData["Title"] = "Details";
      Layout = "~/Views/Shared/_AdminDashboard.cshtml";
}



<div>
    <h4>Store</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.StoreName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.StoreName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.MonthlyFee)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MonthlyFee)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Imagepath)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Imagepath)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Category)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Category.CategoryName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary" style="margin-top:20px;">Edit</a> |
    <a asp-action="Index" class="btn btn-primary" style="margin-top:20px;">Back to store List</a>
</div>
