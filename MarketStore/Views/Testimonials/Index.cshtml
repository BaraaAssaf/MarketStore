@model IEnumerable<MarketStore.Models.Testimonial>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminDashboard.cshtml";
}

<h1>Index</h1>

<p>
    <a hidden asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Masseage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StatusTestimonials)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Masseage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StatusTestimonials)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.Fname)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a>   
            </td>
        </tr>
}
    </tbody>
</table>
